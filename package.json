{
  "name": "canvaboy",
  "version": "1.0.0",
  "description": "Gentle GitHub PR review notifier",
  "main": "dist/main.js",
  "scripts": {
    "compile": "node_modules/.bin/tsc -p .",
    "prestart": "npm run compile",
    "start": "./node_modules/.bin/electron dist/main.js",
    "release": "npm run compile && node make-package.js"
  },
  "keywords": [
    "electron",
    "github"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "@types/auto-launch": "^5.0.0",
    "@types/electron": "^1.6.10",
    "@types/electron-store": "^1.3.0",
    "@types/lodash": "^4.14.118",
    "electron": "^3.0.4",
    "electron-packager": "^12.2.0"
  },
  "dependencies": {
    "auto-launch": "^5.0.5",
    "electron-store": "^2.0.0",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "moment-business-time": "^0.7.1",
    "moment-timezone": "^0.5.23",
    "octonode": "^0.9.5",
    "simple-git": "^1.107.0",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.6"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "printWidth": 120
  }
}
